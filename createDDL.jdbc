CREATE TABLE items (id INTEGER NOT NULL, created_at TIMESTAMP, description VARCHAR(255), name VARCHAR(255), object_name VARCHAR(255) UNIQUE, price DECIMAL(38), updated_at TIMESTAMP, version INTEGER, PRIMARY KEY (id));
CREATE TABLE users (id INTEGER NOT NULL, created_at TIMESTAMP, login_id VARCHAR(255) UNIQUE, name VARCHAR(255), password VARCHAR(255), role INTEGER, updated_at TIMESTAMP, version INTEGER, PRIMARY KEY (id));
CREATE TABLE SEQUENCE (SEQ_NAME VARCHAR(50) NOT NULL, SEQ_COUNT DECIMAL(38), PRIMARY KEY (SEQ_NAME));
